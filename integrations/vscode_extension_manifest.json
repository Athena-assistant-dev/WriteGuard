  "version": "0.0.2",
  "publisher": "yourpublisher",
  "engines": {
    "vscode": ">=1.70.0"
  },
  "categories": ["Other", "AI", "Utilities"],
  "activationEvents": [
    "onCommand:writeguard.runSafeWrite"
