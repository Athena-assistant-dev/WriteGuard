  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "writeguard.runSafeWrite",
        "title": "Run Safe Write with WriteGuard"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.7.4",
    "vscode": "^1.1.37"
  }
}